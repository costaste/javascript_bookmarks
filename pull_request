javascript:(() => {const ta = document.createElement('pre');ta.innerText = [...document.getElementsByClassName('file-header')].map((el) => el.querySelector('.file-info').querySelector('a').innerText).reduce((acc,relativePath) => { return acc += `| \`${relativePath}\`\n|:----\n| Change...\n\n`; }, '');document.body.appendChild(ta);if (document.body.createTextRange) {const range = document.body.createTextRange();range.moveToElement(ta);range.select();} else if (window.getSelection) {const selection = window.getSelection();const range = document.createRange();range.selectNodeContents(ta);selection.removeAllRanges();selection.addRange(range);} else {window.alert("Error selecting from range");}document.execCommand('copy');ta.remove();})();

